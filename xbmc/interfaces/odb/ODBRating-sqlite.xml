<changelog xmlns="http://www.codesynthesis.com/xmlns/odb/changelog" database="sqlite" version="1">
  <model version="1">
    <table name="rating" kind="object">
      <column name="idRating" type="INTEGER" null="false"/>
      <column name="file" type="INTEGER" null="true"/>
      <column name="ratingType" type="TEXT" null="false"/>
      <column name="rating" type="REAL" null="true"/>
      <column name="votes" type="INTEGER" null="false"/>
      <primary-key auto="true">
        <column name="idRating"/>
      </primary-key>
      <foreign-key name="file_fk" deferrable="DEFERRED">
        <column name="file"/>
        <references table="file">
          <column name="idFile"/>
        </references>
      </foreign-key>
      <index name="rating_file_i">
        <column name="file"/>
      </index>
      <index name="rating_ratingType_i">
        <column name="ratingType"/>
      </index>
      <index name="rating_rating_i">
        <column name="rating"/>
      </index>
    </table>
  </model>
</changelog>
