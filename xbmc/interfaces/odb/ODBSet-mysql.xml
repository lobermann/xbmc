<changelog xmlns="http://www.codesynthesis.com/xmlns/odb/changelog" database="mysql" version="1">
  <model version="1">
    <table name="set" options="ENGINE=InnoDB" kind="object">
      <column name="idSet" type="BIGINT UNSIGNED" null="false"/>
      <column name="name" type="TEXT" null="false"/>
      <column name="overview" type="TEXT" null="false"/>
      <primary-key auto="true">
        <column name="idSet"/>
      </primary-key>
      <index name="name_i">
        <column name="name"/>
      </index>
    </table>
    <table name="set_artwork" options="ENGINE=InnoDB" kind="container">
      <column name="object_id" type="BIGINT UNSIGNED" null="false"/>
      <column name="index" type="BIGINT UNSIGNED" null="false"/>
      <column name="value" type="BIGINT UNSIGNED" null="true"/>
      <foreign-key name="set_artwork_object_id_fk" on-delete="CASCADE">
        <column name="object_id"/>
        <references table="set">
          <column name="idSet"/>
        </references>
      </foreign-key>
      <index name="object_id_i">
        <column name="object_id"/>
      </index>
      <index name="index_i">
        <column name="index"/>
      </index>
      <foreign-key name="set_artwork_value_fk" deferrable="DEFERRED">
        <column name="value"/>
        <references table="art">
          <column name="idArt"/>
        </references>
      </foreign-key>
    </table>
  </model>
</changelog>
