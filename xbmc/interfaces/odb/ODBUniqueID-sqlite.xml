<changelog xmlns="http://www.codesynthesis.com/xmlns/odb/changelog" database="sqlite" version="1">
  <model version="1">
    <table name="uniqueid" kind="object">
      <column name="idUniqueID" type="INTEGER" null="false"/>
      <column name="file" type="INTEGER" null="true"/>
      <column name="value" type="TEXT" null="false"/>
      <column name="type" type="TEXT" null="false"/>
      <primary-key auto="true">
        <column name="idUniqueID"/>
      </primary-key>
      <foreign-key name="file_fk" deferrable="DEFERRED">
        <column name="file"/>
        <references table="file">
          <column name="idFile"/>
        </references>
      </foreign-key>
      <index name="uniqueid_file_i">
        <column name="file"/>
      </index>
      <index name="uniqueid_type_i">
        <column name="type"/>
      </index>
      <index name="uniqueid_value_i">
        <column name="value"/>
      </index>
    </table>
  </model>
</changelog>
